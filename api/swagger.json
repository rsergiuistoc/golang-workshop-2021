{
    "swagger": "2.0",
    "info": {
        "description": "Swagger API for Workshop Todo-Service.",
        "title": "Workshop Todo-Service Api",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
